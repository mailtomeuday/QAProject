        <fullName>Set_Opportunity_Identifier</fullName>
        <field>Opportunity_Identifier__c</field>
        <formula>LEFT(  Account.Name &amp; &quot; - &quot; &amp;  TEXT(Revenue_Type__c)  &amp; &quot; - &quot; &amp; TEXT(YEAR( CloseDate )) &amp; &quot; - &quot; &amp; Opportunity_Number__c  , 255)</formula>
