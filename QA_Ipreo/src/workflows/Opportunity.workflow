        <active>true</active>
        <description>Opportunity:
IF Revenue Delta &lt; 0, THEN Revenue Delta Type = &quot;Downtick&quot;</description>
        <formula>Net_New_Business__c &lt; 0</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>%5BOpportunity%5D Set Net New Business to Flat</fullName>
        <actions>
            <name>Set_Net_New_Business_to_Flat</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Opportunity:
IF Revenue Delta &lt; 0, THEN Revenue Delta Type = &quot;Flat&quot;</description>
        <formula>Net_New_Business__c = 0</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>%5BOpportunity%5D Set Net New Business to Uptick</fullName>
        <actions>
            <name>Set_Net_New_Business_to_Uptick</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Opportunity:
IF Revenue Delta &gt; 0, THEN Revenue Delta Type = &quot;Uptick&quot;</description>
        <formula>Net_New_Business__c &gt; 0</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>%5BOpportunity%5D Set Opportunity Name</fullName>
        <actions>
            <name>Set_Opportunity_Name</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>IsClosed</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>%5BOpportunity%5D Set Renewal Status to Cancel</fullName>
        <actions>
            <name>Set_Renewal_Status_to_Cancel</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>IF Record Type = &quot;Renewal&quot;, THEN
IF Lost, THEN Renewal Status = &quot;Cancel&quot;</description>
        <formula>AND( RecordType.DeveloperName = &apos;Renewal&apos;     ,NOT(CONTAINS(TEXT(StageName), &quot;Won&quot;))     ,IsClosed   )</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>%5BOpportunity%5D Set Renewal Status to Full</fullName>
        <actions>
            <name>Set_Renewal_Status_to_Full</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>IF Record Type = &quot;Renewal&quot;, THEN
IF Won AND No. of Renewed Opportunity Products = No. of Opportunity Products, THEN Renewal Status = &quot;Full&quot;</description>
        <formula>AND( RecordType.DeveloperName = &apos;Renewal&apos;     , CONTAINS(TEXT(StageName), &quot;Won&quot;)     , No_of_Renewed_Opp_Products__c =  No_of_Opportunity_Products__c  )</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>%5BOpportunity%5D Set Renewal Status to Null</fullName>
        <actions>
            <name>Set_Renewal_Status_to_Null</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>IF Record Type = &quot;New Business&quot;, THEN Renewal Status = &quot;&quot;.</description>
        <formula>RecordType.DeveloperName = &apos;New_Business&apos;</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>%5BOpportunity%5D Set Renewal Status to Partial</fullName>
        <actions>
            <name>Set_Renewal_Status_to_Partial</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>IF Record Type = &quot;Renewal&quot;, THEN
IF Won AND No. of Renewed Opportunity Products â‰  No. of Opportunity Products, THEN Renewal Status = &quot;Partial&quot;</description>
        <formula>AND( RecordType.DeveloperName = &apos;Renewal&apos;     , CONTAINS(TEXT(StageName), &quot;Won&quot;), No_of_Renewed_Opp_Products__c !=  No_of_Opportunity_Products__c)</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>%5BOpportunity%5D Set Renewal Status to Renewal Pending</fullName>
        <actions>
            <name>Set_Renewal_Status_to_Renewal_Pending</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>IF Record Type = &quot;Renewal&quot;, THEN
IF NOT(Closed), THEN Renewal Status = &quot;Renewal Pending&quot;</description>
        <formula>AND( RecordType.DeveloperName = &apos;Renewal&apos;     ,NOT( IsClosed ) )</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>%5BOpportunity%5D Set Verified Date to Today</fullName>
        <actions>
            <name>Set_Opportunity_Verified_Date_to_Today</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Set Close Date when StageName = &quot;Closed Cancelled Verified&quot; or &quot;Closed Won Verified&quot;</description>
        <formula>ISCHANGED(StageName) &amp;&amp; (  ISPICKVAL(StageName, &quot;Closed Won Verified&quot;)  || ISPICKVAL(StageName, &quot;Closed Cancelled Verified&quot;)  )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>%5BOpportunity%5D This Year ACV</fullName>
        <actions>
            <name>Update_This_Year_ACV</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Copy ACV to This Year ACV if close date equals this year</description>
        <formula>OR(ISNEW(), ISCHANGED(CloseDate), ISCHANGED(StageName))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>%5BOpportunity%5D%3ACheck Opp Closed Date in CY</fullName>
        <actions>
            <name>Set_Is_Closed_Date_In_Current_Year</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>ISNEW() || ISCHANGED(CloseDate)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Copy Account  Expected Date to Opportunity Close Date</fullName>
        <actions>
            <name>Copy_Acct_Expected_Date_to_Opty_cls_Date</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Opportunity.Lead_Created_By__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Account.Expected_Date__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName>Copy Description text from custom to standard field</fullName>
        <actions>
            <name>Copy_Descriptionfrom_custom_to_stndrd_f</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Opportunity.Lead_Created_By__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <triggerType>onCreateOnly</triggerType>
    </rules>
</Workflow>
