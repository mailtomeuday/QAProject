        <fullName>%5BOpportunity%5D Set Close Date to Today</fullName>
        <actions>
            <name>Set_opportunity_close_date_to_today</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>ISCHANGED(StageName) &amp;&amp; ( ISPICKVAL(StageName, &quot;Closed Won Sales&quot;)  || ISPICKVAL(StageName, &quot;Closed Lost&quot;)  || ISPICKVAL(StageName, &quot;Closed Cancelled&quot;) )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
