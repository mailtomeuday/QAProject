    <fieldUpdates>
        <fullName>Update_This_Year_ACV</fullName>
        <description>Item 401: Closed Won Verifed Sales To Date - a roll up of all ACV on Opp but just for Closed Date=This Year.
if Closed Date=This Year, update this field as 1, then use to roll-up ACV</description>
        <field>This_Year_ACV__c</field>
        <formula>IF(AND((YEAR(CloseDate)  ==  YEAR(TODAY())), OR(ISPICKVAL(StageName, &apos;Closed Won Sales&apos;), ISPICKVAL(StageName, &apos;Closed Won Verified&apos;))), 1, 0)</formula>
        <name>Update This Year ACV</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Formula</operation>
        <protected>false</protected>
    </fieldUpdates>
