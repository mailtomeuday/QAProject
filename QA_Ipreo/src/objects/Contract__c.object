    <validationRules>
        <fullName>Billing_Acnt_req_at_Status_Cus_and_Fully</fullName>
        <active>true</active>
        <description>Billing Account required at Status &quot;Customer Executed&quot; or &quot;Fully Executed&quot;</description>
        <errorConditionFormula>ISBLANK( Billing_Account__c )  &amp;&amp;  
(ISPICKVAL( Status__c , &apos;Customer Executed&apos;)  || 
ISPICKVAL( Status__c , &apos;Fully Executed&apos;))</errorConditionFormula>
        <errorDisplayField>Billing_Account__c</errorDisplayField>
        <errorMessage>Billing Account required at Status &quot;Customer Executed&quot; or &quot;Fully Executed&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Readonly_When_Fully_Executed</fullName>
        <active>true</active>
        <description>Using custom permission, Edit_Fully_Executed_Contract, because some permission sets need the ability to edit fully executed contracts, and validation rules cannot reference permission sets.</description>
        <errorConditionFormula>ISPICKVAL( PRIORVALUE(Status__c), &apos;Fully Executed&apos;)  &amp;&amp; 
$Profile.Name != &apos;System Administrator&apos; &amp;&amp; NOT ( $Permission.Edit_Fully_Executed_Contract )</errorConditionFormula>
        <errorMessage>Cannot edit fully executed contracts.  Please ask the Contracts or Sales Ops team to make any necessary changes.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Req_Approval_Date_When_Fully_Executed</fullName>
        <active>false</active>
        <errorConditionFormula>ISPICKVAL( Status__c, &quot;Fully Executed&quot;) &amp;&amp; ISBLANK( Contracts_Team_Approval_Date__c )</errorConditionFormula>
        <errorDisplayField>Contracts_Team_Approval_Date__c</errorDisplayField>
        <errorMessage>Contracts Team Approval Date required when contract is fully executed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Req_Approver_when_fully_executed</fullName>
        <active>false</active>
        <errorConditionFormula>ISPICKVAL( Status__c , &quot;Fully Executed&quot;) &amp;&amp; ISBLANK( Contracts_Team_Approver__c )</errorConditionFormula>
        <errorDisplayField>Contracts_Team_Approver__c</errorDisplayField>
        <errorMessage>Contracts Team Approver required when contract is fully executed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Req_Attachment_When_Executed</fullName>
        <active>true</active>
        <errorConditionFormula>(No_of_Attachments__c = 0 ||  ISBLANK(No_of_Attachments__c) )
&amp;&amp; (ISPICKVAL( Status__c , &apos;Customer Executed&apos;) || ISPICKVAL( Status__c , &apos;Fully Executed&apos;))</errorConditionFormula>
        <errorMessage>Attachment required to execute this contract</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Req_Billing_City_When_Executed</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( Billing_City__c ) &amp;&amp; 
( 
ISPICKVAL( Status__c , &apos;Customer Executed&apos;) || 
ISPICKVAL( Status__c , &apos;Fully Executed&apos;) 
)</errorConditionFormula>
        <errorDisplayField>Billing_City__c</errorDisplayField>
        <errorMessage>Billing City required to execute this contract</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Req_Billing_Contact_When_Executed</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(Billing_Contact__c)  &amp;&amp; 
( 
ISPICKVAL( Status__c , &apos;Customer Executed&apos;) || 
ISPICKVAL( Status__c , &apos;Fully Executed&apos;) 
)</errorConditionFormula>
        <errorDisplayField>Billing_Contact__c</errorDisplayField>
        <errorMessage>Billing Contact required to execute this contract</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Req_Billing_Country_When_Executed</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(Billing_Country__c)  &amp;&amp; 
( 
ISPICKVAL( Status__c , &apos;Customer Executed&apos;) || 
ISPICKVAL( Status__c , &apos;Fully Executed&apos;) 
)</errorConditionFormula>
        <errorDisplayField>Billing_Country__c</errorDisplayField>
        <errorMessage>Billing Country required to execute this contract</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Req_Billing_Frequency_Other_Please</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Billing_Frequency__c , &apos;Other&apos; ) &amp;&amp;  ISBLANK( Billing_Frequency_Other_Please_Specify__c )</errorConditionFormula>
