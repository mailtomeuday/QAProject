        <formula>Work_Order__r.Completion_Date__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Work Order Completion Date</label>
        <required>false</required>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Work_Order_Est_Completion_Date__c</fullName>
