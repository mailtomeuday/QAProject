    <fields>
        <fullName>Asset__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>Asset associated with Opportunity</description>
        <externalId>false</externalId>
        <label>Asset</label>
        <lookupFilter>
            <active>true</active>
            <filterItems>
                <field>Asset.AccountId</field>
                <operation>equals</operation>
                <valueField>$Source.Account__c</valueField>
            </filterItems>
            <filterItems>
                <field>Asset.Product2Id</field>
                <operation>equals</operation>
                <valueField>$Source.Product__c</valueField>
            </filterItems>
            <isOptional>true</isOptional>
        </lookupFilter>
        <referenceTo>Asset</referenceTo>
        <relationshipLabel>Opportunity Products</relationshipLabel>
        <relationshipName>Opportunity_Products</relationshipName>
        <required>false</required>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Backlog_Days__c</fullName>
        <externalId>false</externalId>
        <formula>IF(Backlog__c, 
	IF(ISPICKVAL(Opportunity.ForecastCategoryName, &quot;Projected&quot;), Subscription_Start_Date__c - TODAY(), Subscription_Start_Date__c -  Revenue_Recognition_Date__c), 
	NULL
)</formula>
        <label>Backlog Days</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Backlog__c</fullName>
        <externalId>false</externalId>
        <formula>IF(ISPICKVAL(Product_Transaction_Type__c, &quot;Renewal&quot;) &amp;&amp; Subscription_Start_Date__c &lt; DATE(YEAR(TODAY()), MONTH(TODAY()), 01), TRUE, FALSE)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Backlog</label>
        <type>Checkbox</type>
    </fields>
