    </fields>
    <fields>
        <fullName>Subtotal</fullName>
    </fields>
    <fields>
        <fullName>Term_Months__c</fullName>
        <description>Opportunity Term in Months</description>
        <externalId>false</externalId>
        <inlineHelpText>Term of subscription, INCLUSIVE of any free period.  Leave blank for non-subscription products.  If entered, Subscription Start Date and Subscription End Date must also be entered.</inlineHelpText>
        <label>Term (months)</label>
        <precision>2</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>TotalPrice</fullName>
    </fields>
    <fields>
        <fullName>UnitPrice</fullName>
        <inlineHelpText>Total Contracted Product Value - the Product level breakout for Total Contracted Value (TCV).  Will be different from Annual Contracted Product Value (ACpV) when term is greater than one year.</inlineHelpText>
    </fields>
    <fields>
        <fullName>Upfront_Expense_Fee__c</fullName>
        <externalId>false</externalId>
