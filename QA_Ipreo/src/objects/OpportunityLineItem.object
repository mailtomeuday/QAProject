        <formula>Work_Order__r.Est_Completion_Date__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Work Order Est Completion Date</label>
        <required>false</required>
        <type>Date</type>
    </fields>
    <fields>
