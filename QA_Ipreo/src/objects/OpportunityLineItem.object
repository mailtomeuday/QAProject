    </fields>
    <fields>
        <fullName>Status__c</fullName>
        <description>The values of Won/Lost pertain to New Business opportunities and the values of Cancelled/Renewed pertain to Renewal opportunities.</description>
        <externalId>false</externalId>
        <inlineHelpText>Required when Opportunity Stage is Closed%.  When Status has not been populated, most fields on page are not validated by system.  Once Status is populated (with either Won/Lost for New Business or Renewed/Cancelled for Renewals), the validations occur.</inlineHelpText>
        <label>Status</label>
        <picklist>
            <controllingField>Product_Transaction_Type__c</controllingField>
            <picklistValues>
                <fullName>Won</fullName>
                <default>false</default>
                <controllingFieldValues>Add On</controllingFieldValues>
                <controllingFieldValues>New Business</controllingFieldValues>
                <controllingFieldValues>Upsell</controllingFieldValues>
            </picklistValues>
            <picklistValues>
                <fullName>Lost</fullName>
                <default>false</default>
                <controllingFieldValues>Add On</controllingFieldValues>
                <controllingFieldValues>New Business</controllingFieldValues>
                <controllingFieldValues>Upsell</controllingFieldValues>
            </picklistValues>
            <picklistValues>
                <fullName>Renewed</fullName>
                <default>false</default>
                <controllingFieldValues>Renewal</controllingFieldValues>
            </picklistValues>
            <picklistValues>
                <fullName>Cancelled</fullName>
                <default>false</default>
                <controllingFieldValues>Renewal</controllingFieldValues>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <required>false</required>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>Subgroup__c</fullName>
        <externalId>false</externalId>
        <formula>Text( PricebookEntry.Product2.Subgroup__c )</formula>
        <label>Subgroup</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Subscription_End_Date__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Applies to Opportunity Products sold as a subscription, only (N/A for projects).  Subscription End Date must be same (coterminous) for all Opportunity Products on the Opportunity.  Req at Won, Renewed if Subscription Start Date and Term populated.</inlineHelpText>
        <label>Subscription End Date</label>
        <required>false</required>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Subscription_Start_Date__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Applies to Opportunity Products sold as a subscription, only (N/A for projects).  Required at Status &apos;Won&apos; or &apos;Renewed&apos; if Subscription End Date and Term are populated.</inlineHelpText>
        <label>Subscription Start Date</label>
        <required>false</required>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Subscription_Start_Date_is_Current_Mo__c</fullName>
        <description>Used primarily for ExComm report filtering in Wave</description>
        <externalId>false</externalId>
        <formula>Subscription_Start_Date__c  &gt;= DATE(YEAR(TODAY()),MONTH(TODAY()),05)</formula>
        <label>Subscription Start Date is &gt;= Current Mo</label>
