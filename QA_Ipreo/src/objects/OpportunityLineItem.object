        <externalId>false</externalId>
        <formula>Work_Order_Completion_Date__c &gt;= DATE(YEAR(TODAY()),MONTH(TODAY()),05)</formula>
        <label>Work Order Comp Date is &gt;= Current Month</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Work_Order_Completion_Date__c</fullName>
