    <fields>
        <fullName>Master_ACV_Eligible__c</fullName>
        <description>Formula Logic:
=IF(Product Revenue Type = ACV or ACV/TRANS,
IF(Today’s Date is &gt;= Subscription Start Date AND &lt;= Subscription End Date, “TRUE”,
IF(Forecast Category = Projected AND Product Transaction Type = Renewal, “TRUE”, “FALSE”</description>
        <externalId>false</externalId>
        <formula>IF(OR(ISPICKVAL(Product_Revenue_Type__c, &quot;ACV&quot;), ISPICKVAL(Product_Revenue_Type__c, &quot;ACV/TRANS&quot;)), 
	IF(AND(Opportunity.RecordType.Name = &quot;Renewal&quot;, ISBLANK(Opportunity.Original_Opportunity__c), ISPICKVAL(Opportunity.ForecastCategoryName, &quot;Projected&quot;)), 
		TRUE, 
		IF(AND(Opportunity.RecordType.Name = &quot;Renewal&quot;, ISBLANK(Opportunity.Original_Opportunity__c), ISPICKVAL(Opportunity.ForecastCategoryName, &quot;Contracted&quot;), Subscription_Start_Date__c &gt; TODAY()), 
			TRUE, 
			IF(AND(Opportunity.RecordType.Name = &quot;Renewal&quot;, ISPICKVAL(Status__c, &quot;Cancelled&quot;), Subscription_Start_Date__c &lt;= TODAY(), Subscription_End_Date__c &gt;= TODAY()),
				TRUE, 
				IF(AND(Subscription_Start_Date__c &lt;= TODAY(), Subscription_End_Date__c &gt;= TODAY(), ISPICKVAL(Opportunity.ForecastCategoryName, &quot;Contracted&quot;)), 
					TRUE, 
					FALSE
				) 
			) 
		) 
	), 
	FALSE 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Master ACV Eligible</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Master_ACV_Value__c</fullName>
        <externalId>false</externalId>
        <formula>IF(OR(ISPICKVAL(Product_Revenue_Type__c, &quot;ACV&quot;), ISPICKVAL(Product_Revenue_Type__c, &quot;ACV/TRANS&quot;)), 
IF(AND(Opportunity.RecordType.Name = &quot;Renewal&quot;, ISBLANK(Opportunity.Original_Opportunity__c), ISPICKVAL(Opportunity.ForecastCategoryName, &quot;Projected&quot;)), Prior_Year_ACpV__c - Prior_Year_Pass_Through__c, 
IF(AND(Opportunity.RecordType.Name = &quot;Renewal&quot;, ISBLANK(Opportunity.Original_Opportunity__c), ISPICKVAL(Opportunity.ForecastCategoryName, &quot;Contracted&quot;), Subscription_Start_Date__c &gt; TODAY()),  Prior_Year_ACpV__c - Prior_Year_Pass_Through__c, 
IF(AND(Opportunity.RecordType.Name = &quot;Renewal&quot;, ISPICKVAL(Status__c, &quot;Cancelled&quot;), Subscription_Start_Date__c &lt;= TODAY(), Subscription_End_Date__c &gt;= TODAY()), Net_New_Business__c,
IF(AND(Subscription_Start_Date__c &lt;= TODAY(), Subscription_End_Date__c &gt;= TODAY(), ISPICKVAL(Opportunity.ForecastCategoryName, &quot;Contracted&quot;)),  Net_ACpV__c, 
0 
) 
)
) 
), 
0 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
