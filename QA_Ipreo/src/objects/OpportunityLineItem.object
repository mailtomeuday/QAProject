        <externalId>false</externalId>
        <formula>IF(
	CONTAINS(TEXT(Opportunity.StageName), &quot;Verified&quot;), 
	IF(
		NOT(ISPICKVAL(Product_Revenue_Type__c, &quot;PROJ&quot;)),
		IF(
			AND(
   				NOT( ISBLANK(Opportunity.CloseDate) ) ,
   				NOT( ISBLANK(Subscription_Start_Date__c) )
  			),
   			IF(
				Opportunity.CloseDate &gt; Subscription_Start_Date__c, 
     				Opportunity.CloseDate,
     				Subscription_Start_Date__c
     			), 
  			NULL
		),
		Work_Order__r.Completion_Date__c
