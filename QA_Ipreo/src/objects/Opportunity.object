        <fullName>Owner_Closed_Date__c</fullName>
        <externalId>false</externalId>
        <formula>Owner.LastName + &apos;, &apos; + 
Owner.FirstName + &apos; - &apos; +  
Text(YEAR(CloseDate)) + &apos;-&apos; +  
IF(
	LEN(Text(Month(CloseDate))) = 1 , 
	(&apos;0&apos; + Text(Month(CloseDate))), 
