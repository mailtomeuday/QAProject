        <errorConditionFormula>And($Profile.Name != &apos;System Administrator&apos; ,Invoice_Unpaid__c, ISPICKVAL(StageName,&apos;Closed Won Verified&apos;))</errorConditionFormula>
        <errorDisplayField>Invoice_Unpaid__c</errorDisplayField>
        <errorMessage>Cannot Close Won Verify Opportunity Since Invoice is Unpaid</errorMessage>
