    <validationRules>
        <fullName>Cannot_Cancel_Opp_with_Renewed_Product</fullName>
        <active>true</active>
        <description>Can not close cancel an renewal opportunity with any product which is renewed. The opportunity products should be cancelled.</description>
        <errorConditionFormula>$Profile.Name != &apos;System Administrator&apos; &amp;&amp; 
RecordType.DeveloperName = &apos;Renewal&apos; &amp;&amp;
( ISPICKVAL( StageName , &apos;Closed Cancelled&apos;)   ||  ISPICKVAL( StageName , &apos;Closed Cancelled Verified&apos;))&amp;&amp;
No_of_Renewed_Opp_Products__c != 0</errorConditionFormula>
        <errorDisplayField>StageName</errorDisplayField>
