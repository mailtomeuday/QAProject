        <errorConditionFormula>NOT( $Permission.By_Pass_Opportunity_Validations ) &amp;&amp;
IsClosed &amp;&amp; CONTAINS(TEXT(StageName), &quot;Won&quot;) &amp;&amp; (
    ISBLANK( Contract__c ) ||
    NOT( ISPICKVAL( Contract__r.Status__c , &quot;Customer Executed&quot; ) ) &amp;&amp;
    NOT( ISPICKVAL( Contract__r.Status__c , &quot;Fully Executed&quot; ) )
)</errorConditionFormula>
