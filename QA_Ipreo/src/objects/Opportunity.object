    <webLinks>
        <fullName>Add_Contract</fullName>
        <availability>online</availability>
        <description>Creates new contract and associates contract to opportunity</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Add Contract</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/31.0/connection.js&quot;)}

// Constants
var CONTRACT_OWNERID = &apos;00Gj0000000a8t4EAA&apos;;

// Check for pre-existing contract
if({!NOT(ISBLANK( Opportunity.Contract__c ))}) {
    alert(&apos;{!$Label.Add_Contract_Contract_Already_Exists}&apos;);

// Prompt for contract creation
} else {

    // Query database to both get the opportunity record type name and get an opportunity record to update
    result = sforce.connection.query(&quot;SELECT Id, Contract__c, RecordType.Name FROM Opportunity WHERE Opportunity.Id = &apos;{!Opportunity.Id}&apos;&quot;);

    // Query Account name since opportunity.Account has the issue with single quote
    resultAcc = sforce.connection.query(&quot;SELECT Id, Contract__c, Account.Name FROM Opportunity WHERE Opportunity.Id = &apos;{!Opportunity.Id}&apos;&quot;);

    // If query successful, create contract record with pre-populated fields
    if(result.getArray(&quot;records&quot;).length == 1) {
        var oppty        = result.getArray(&quot;records&quot;)[0];
        var opptyWithAcc = resultAcc.getArray(&quot;records&quot;)[0];
        var cont         = new sforce.SObject(&apos;Contract__c&apos;);
        var newName      = (oppty.RecordType.Name == &apos;New Business&apos;) ? &apos;New &apos; : &apos;&apos;;

        // Truncate name to 80 characters to accommodate the 80-char field length
        cont.Name        = {!JSENCODE(&apos;opptyWithAcc.Account.Name&apos;)} + &apos; &apos; + &apos;{!Opportunity.Product_Bundle__c} &apos; + newName + &apos;{!YEAR(TODAY())}&apos;;
        cont.Name        = cont.Name.substring(0, 80);

        cont.OwnerId     = CONTRACT_OWNERID;
        cont.Billing_Account__c = &apos;{! Opportunity.AccountId }&apos;;
        cont.Account__c  = &apos;{!Opportunity.AccountId}&apos;;
        result           = sforce.connection.create([cont]);

        // If contract created, update opportunity with lookup to new contract record
        if(result[0].success == &apos;true&apos;) {
            cont.Id = result[0].id;
            oppty.Contract__c = cont.Id;
            delete oppty[&quot;RecordType&quot;];
            result = sforce.connection.update([oppty]);

            // If opportunity updated, redirect to contract edit page with a return URL to opportunity
            if(result[0].success == &apos;true&apos;) {
                window.location.href = &apos;/&apos; + cont.Id + &apos;/e?retURL=&apos; + &quot;{!URLENCODE(&apos;/&apos;)}&quot; + cont.Id;

            // Error updating opportunity
            } else {
                alert(&apos;{!$Label.Add_Contract_Error_Update_Oppty}&apos; + &apos;\n\n&apos; + result[0].errors.message);
            }

        // Error creating contract
        } else {
            alert(&apos;{!$Label.Add_Contract_Error_Creating_Contract}&apos; + &apos;\n\n&apos; + result[0].errors.message);
        }

    // Error querying database for opportunity record
    } else {
        alert(&apos;{!$Label.Add_Contract_Error_Query_Database}&apos;);
    }
}</url>
    </webLinks>
</CustomObject>
