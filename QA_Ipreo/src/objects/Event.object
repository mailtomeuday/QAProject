    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>Req_Name_When_Type_Is_Not_Acct_Intellig</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(WhoId) &amp;&amp;  NOT(ISPICKVAL( Type , &quot;Account Intelligence&quot;)) &amp;&amp;  NOT(ISPICKVAL( Type , &quot;Email&quot;))</errorConditionFormula>
        <errorDisplayField>WhoId</errorDisplayField>
        <errorMessage>Name required when Type is not Account Intelligence</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Products</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( Products__c )&amp;&amp; NOT(Created_From_Plugin__c)</errorConditionFormula>
        <errorDisplayField>Products__c</errorDisplayField>
        <errorMessage>You must select at least one product</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Related_To</fullName>
        <active>true</active>
        <description>Require &quot;Related To&quot; field for &quot;Meeting&quot; activity types.</description>
        <errorConditionFormula>BEGINS(TEXT(Type), &quot;Meeting (Sales&quot;) &amp;&amp; (ISBLANK(WhatId) || NOT(BEGINS(WhatId, &quot;006&quot;))) &amp;&amp; $Profile.Name != &quot;System Administrator&quot; &amp;&amp; $Profile.Name != &quot;Operations User&quot;</errorConditionFormula>
        <errorDisplayField>WhatId</errorDisplayField>
        <errorMessage>The &quot;Related To&quot; field is required AND must be an Opportunity for Sales Meeting activity types.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Send_Alert</fullName>
        <active>true</active>
        <description>Require &quot;Send Alert&quot; = Yes for &quot;Meeting&quot; activity types.</description>
        <errorConditionFormula>BEGINS(TEXT(Type), &quot;Meeting (Sales&quot;) &amp;&amp; NOT(ISPICKVAL(Send_Alert__c, &quot;Yes&quot;)) &amp;&amp; $Profile.Name != &quot;System Administrator&quot; &amp;&amp; $Profile.Name != &quot;Operations User&quot;</errorConditionFormula>
        <errorDisplayField>Send_Alert__c</errorDisplayField>
        <errorMessage>The &quot;Send Alert&quot; value must be &quot;Yes&quot; for Sales Meeting activity types.</errorMessage>
    </validationRules>
</CustomObject>
