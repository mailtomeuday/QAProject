    <validationRules>
        <fullName>Please_Select_an_Alert_group</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(Send_Alert__c, &apos;Yes&apos;)
&amp;&amp; (ISBLANK( Alert_Coverage_Groups__c) &amp;&amp; ISBLANK(  Alert_Individual_Users__c ))</errorConditionFormula>
        <errorDisplayField>Alert_Coverage_Groups__c</errorDisplayField>
        <errorMessage>Please select an alert group</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Req_Name_When_Type_Is_Not_Acct_Intellig</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(WhoId) &amp;&amp;  NOT(ISPICKVAL( Type , &quot;Account Intelligence&quot;)) &amp;&amp;  NOT(ISPICKVAL( Type , &quot;Email&quot;))
&amp;&amp; NOT(Created_From_Plugin__c)</errorConditionFormula>
        <errorDisplayField>WhoId</errorDisplayField>
        <errorMessage>Name required when Type is not Account Intelligence</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Products</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( Products__c ) &amp;&amp; NOT(ISPICKVAL(Type , &apos;Call (Not Connected)&apos;) || ISPICKVAL(Type , &apos;Call (Voicemail)&apos;)) &amp;&amp; NOT(ISPICKVAL(Type , &apos;Email&apos;)) &amp;&amp; NOT(ISBLANK( TEXT( Type ) ))
&amp;&amp; NOT(Created_From_Plugin__c)</errorConditionFormula>
        <errorDisplayField>Products__c</errorDisplayField>
        <errorMessage>You must select at least one product</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Related_To</fullName>
        <active>true</active>
        <description>Require &quot;Related To&quot; field for &quot;Proposal Sent&quot; and &quot;Contract Sent&quot; activity types.  The selected item must be an Opportunity</description>
        <errorConditionFormula>(ISPICKVAL(Type, &quot;Proposal Sent&quot;) || ISPICKVAL(Type, &quot;Contract Sent&quot;)) 
&amp;&amp; (ISBLANK(WhatId) || NOT(BEGINS(WhatId, &quot;006&quot;))) &amp;&amp; $Profile.Name != &quot;System Administrator&quot; &amp;&amp; $Profile.Name != &quot;Operations User&quot;</errorConditionFormula>
        <errorDisplayField>WhatId</errorDisplayField>
        <errorMessage>The &quot;Related To&quot; field is required AND must be an Opportunity for &quot;Proposal Sent&quot; and &quot;Contract Sent&quot; activity types.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Send_Alert</fullName>
        <active>true</active>
        <description>Require &quot;Send Alert&quot; = &quot;Yes&quot; for &quot;Proposal Sent&quot; and &quot;Contract Sent&quot; activity types</description>
        <errorConditionFormula>(ISPICKVAL(Type, &quot;Proposal Sent&quot;) || ISPICKVAL(Type, &quot;Contract Sent&quot;)) 
&amp;&amp; NOT(ISPICKVAL(Send_Alert__c, &quot;Yes&quot;)) &amp;&amp; $Profile.Name != &quot;System Administrator&quot; &amp;&amp; $Profile.Name != &quot;Operations User&quot;</errorConditionFormula>
        <errorDisplayField>Send_Alert__c</errorDisplayField>
        <errorMessage>The &quot;Send Alert&quot; value must be &quot;Yes&quot; for &quot;Proposal Sent&quot; and &quot;Contract Sent&quot; activity types.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_User_Relationship</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( TEXT(User_Relationship__c) ) &amp;&amp; NOT(ISPICKVAL(Type , &apos;Call (Not Connected)&apos;) || ISPICKVAL(Type , &apos;Call (Voicemail)&apos;)) &amp;&amp; NOT(ISPICKVAL(Type , &apos;Email&apos;))  &amp;&amp; NOT(ISBLANK( TEXT( Type ) )) &amp;&amp; NOT(Created_From_Plugin__c)</errorConditionFormula>
        <errorDisplayField>User_Relationship__c</errorDisplayField>
        <errorMessage>You must enter a value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Vertical</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( TEXT( Vertical__c ) ) &amp;&amp; NOT(ISPICKVAL(Type , &apos;Call (Not Connected)&apos;) || ISPICKVAL(Type , &apos;Call (Voicemail)&apos;)) &amp;&amp; NOT(ISPICKVAL(Type , &apos;Email&apos;)) &amp;&amp; NOT(ISBLANK( TEXT( Type ) ))
&amp;&amp; NOT(Created_From_Plugin__c)</errorConditionFormula>
        <errorDisplayField>Vertical__c</errorDisplayField>
