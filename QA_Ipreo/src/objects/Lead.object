    <validationRules>
        <fullName>Country_Required_On_Ready_For_Conversion</fullName>
        <active>true</active>
        <description>&quot;Country&quot; field required on Leads when Status = &quot;Ready for Conversion&quot;
SPS-1792</description>
        <errorConditionFormula>ISBLANK(Country) &amp;&amp;  ISPICKVAL(Status, &apos;Ready for Conversion&apos;)</errorConditionFormula>
        <errorDisplayField>Country</errorDisplayField>
        <errorMessage>Country is required when Lead is Ready for Conversion</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Email_Required</fullName>
        <active>true</active>
        <description>Email required for Service User Profile</description>
        <errorConditionFormula>CONTAINS($Profile.Name, &apos;Service User&apos;) &amp;&amp; ISBLANK( Email )</errorConditionFormula>
        <errorDisplayField>Email</errorDisplayField>
        <errorMessage>Please provide valid Email</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LID__Check_CompanyId_Value</fullName>
        <active>true</active>
        <description>Throws an exception message if the company id contains non-numerics</description>
        <errorConditionFormula>AND(
	(LID__LinkedIn_Company_Id__c &lt;&gt; NULL),
	NOT(ISNUMBER(LID__LinkedIn_Company_Id__c))
)</errorConditionFormula>
        <errorDisplayField>LID__LinkedIn_Company_Id__c</errorDisplayField>
        <errorMessage>CompanyId should be a numeric value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Region_Required_On_Lead</fullName>
        <active>false</active>
        <description>Prevent creating and saving Lead without region, but bypass that for Sys Admin so that Ipreo System User can continue to create EDD leads.</description>
        <errorConditionFormula>AND($Profile.Name != &apos;System Administrator&apos;, 
$Profile.Name != &apos;Marketo-Salesforce Sync&apos;,
$Profile.Name != &apos;Operations User&apos;, 
$Profile.Name != &apos;Sales User&apos;, 
ISPICKVAL(Region__c,&apos;&apos;))</errorConditionFormula>
        <errorDisplayField>Region__c</errorDisplayField>
        <errorMessage>Lead region is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Req_Create_Opportunity_Field</fullName>
        <active>false</active>
        <description>Create opportunity field is required at Ready for Conversion.
$Permission.By_Pass_Validation is custom permission which will be assigned to Profiles to by pass this validation.</description>
        <errorConditionFormula>ISBLANK( Text(Create_Opportunity__c) ) &amp;&amp;  NOT($Permission.By_Pass_Validation)</errorConditionFormula>
        <errorDisplayField>Create_Opportunity__c</errorDisplayField>
        <errorMessage>Create Opportunity field is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Verify_Opty_Creation_Create_Opty_field</fullName>
        <active>true</active>
        <description>Validate opportunity creation based on field Create Opportunity on Lead</description>
        <errorConditionFormula>OR(
	( Text(Create_Opportunity__c) ) = &apos;Yes&apos; 
	&amp;&amp; 
	ISBLANK( ConvertedOpportunityId )
	,
	( Text(Create_Opportunity__c) ) = &apos;No&apos; 
	&amp;&amp; 
	NOT(ISBLANK( ConvertedOpportunityId ))
)	
&amp;&amp;  IsConverted</errorConditionFormula>
        <errorMessage>Please check the values of &apos;Create Opportunity&apos; and &apos;Do not create a new opportunity upon conversion&apos; before proceeding</errorMessage>
    </validationRules>
